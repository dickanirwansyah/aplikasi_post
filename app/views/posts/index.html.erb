<h1>All Blog Post</h1>
<br>
  <table border="1">
    <thead>
      <tr>
        <td>Title</td>
        <td>Body</td>
      </tr>
    </thead>
    <% @posts.each do |post| %>
    <tbody>
      <tr>
        <td><%=post.title%></td>
        <td><%=post.body%></td>
      </tr>
    </tbody>
    <% end %>
    <tfoot>
      <tr>
        <td>Title</td>
        <td>Body</td>
      </tr>
    </tfoot>
  </table>
